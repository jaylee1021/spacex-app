    <div class="table-container">
        <table
            class="table is-fullwidth has-sticky-column is-narrow is-striped is-bordered is-hoverable has-sticky-header has-sticky-footer">
            <thead>
                <tr>
                    <th><abbr title="Serial">SN</abbr></th>
                    <th><abbr title="ID">ID</abbr></th>
                    <th><abbr title="Block">Block</abbr></th>
                    <th><abbr title="Status">Status</abbr></th>
                    <th><abbr title="Launches">Launches</abbr></th>
                    <th><abbr title="Reuse Count">Reuse Count</abbr></th>
                    <th><abbr title="RTLS Attempts">RTLS Attempts</abbr></th>
                    <th><abbr title="RTLS Landings">RTLS Landings</abbr></th>
                    <th><abbr title="ASDS Attempts">ASDS Attempts</abbr></th>
                    <th><abbr title="ASDS Landings">ASDS Landings</abbr></th>
                    <th><abbr title="Last Update">Last Update</abbr></th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th><abbr title="Serial">SN</abbr></th>
                    <th><abbr title="ID">ID</abbr></th>
                    <th><abbr title="Block">Block</abbr></th>
                    <th><abbr title="Status">Status</abbr></th>
                    <th><abbr title="Launches">Launches</abbr></th>
                    <th><abbr title="Reuse Count">Reuse Count</abbr></th>
                    <th><abbr title="RTLS Attempts">RTLS Attempts</abbr></th>
                    <th><abbr title="RTLS Landings">RTLS Landings</abbr></th>
                    <th><abbr title="ASDS Attempts">ASDS Attempts</abbr></th>
                    <th><abbr title="ASDS Landings">ASDS Landings</abbr></th>
                    <th><abbr title="Last Update">Last Update</abbr></th>

                </tr>
            </tfoot>
            <tbody>
                <% for( let x=0; x < cores.length; x++ ) { %>
                    <% let core=cores[x] %>
                        <tr>
                            <th>
                                <%= core.serial %>
                            </th>
                            <td>
                                <%= core.id %>
                            </td>
                            <td>
                                <% if (core.block===null) { %>
                                    <%= 'None' %>
                                        <% } else {%>
                                            <%= core.block %>
                                                <% } %>
                            </td>
                            <td>
                                <%= core.status%>
                            </td>
                            <td>
                                <% if (core.launches.length < 1) { %>
                                    <%= 'None' %>
                                        <% } else {%>
                                            <% for( let x=0; x < core.launches.length; x++ ) { %>
                                                <%= core.launches[x] %>
                                                    <br>
                                                    <% } %>
                                                        <% } %>
                            </td>
                            <td>
                                <%= core.reuse_count%>
                            </td>
                            <td>
                                <%= core.rtls_attempts%>
                            </td>
                            <td>
                                <%= core.rtls_landings%>
                            </td>
                            <td>
                                <%= core.asds_attempts%>
                            </td>
                            <td>
                                <%= core.asds_landings%>
                            </td>
                            <td style="max-width: 1000px; word-wrap: break-word;">
                                <% if (core.last_update===null) { %>
                                    <%= 'None' %>
                                        <% } else {%>
                                            <%= core.last_update %>
                                                <% } %>
                            </td>
                        </tr>
                        <% } %>
            </tbody>
        </table>
    </div>








</body>

